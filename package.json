{
  "name": "oats",
  "version": "1.0.0",
  "description": "A slick notes app.",
  "main": "index.js",
  "author": "sidiousvic",
  "license": "UNLICENSED",
  "private": true,
  "proxy": "http://localhost:4000",
  "scripts": {
    "start": "pm2 start server.js",
    "serve": "nodemon server/index.js",
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint --fix --ext .js,.jsx .",
    "migrate": "knex migrate:latest --knexfile models/knexfile.js",
    "seed": "knex seed:run --knexfile models/knexfile.js",
    "rollback": "knex migrate:rollback --knexfile models/knexfile.js"
  },
  "resolutions": {
    "@babel/preset-env": "7.5.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "axios": "^0.20.0",
    "express": "^4.16.2",
    "knex": "^0.20.9",
    "moment": "^2.20.1",
    "morgan": "^1.9.0",
    "pg": "^7.4.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "^3.4.0",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^4.2.3",
    "nodemon": "^2.0.4",
    "prettier": "^1.19.1",
    "pretty-quick": "^1.5.0"
  }
}
