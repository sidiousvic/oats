{
  "name": "oats",
  "version": "1.0.0",
  "description": "A slick notes app.",
  "main": "index.js",
  "author": "sidiousvic",
  "license": "UNLICENSED",
  "private": true,
  "homepage": "https://sidiousvic.dev/oats",
  "proxy": "http://localhost:4000",
  "scripts": {
    "start": "pm2 start server/index.js --name sidiousvic.dev/oats",
    "serve": "ts-node-dev server",
    "dev": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "lint": "eslint --fix --ext .js,.jsx .",
    "migrate": "knex migrate:latest --knexfile models/knexfile.js",
    "seed": "knex seed:run --knexfile models/knexfile.js",
    "rollback": "knex migrate:rollback --knexfile models/knexfile.js"
  },
  "resolutions": {
    "@babel/preset-env": "7.5.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@babel/core": "7.8.4",
    "@svgr/webpack": "4.3.3",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "axios": "^0.20.0",
    "babel-loader": "8.0.6",
    "css-loader": "3.4.2",
    "dotenv": "8.2.0",
    "eslint": "^6.6.0",
    "express": "^4.16.2",
    "file-loader": "4.3.0",
    "html-webpack-plugin": "4.0.0-beta.11",
    "jest": "24.9.0",
    "knex": "^0.20.9",
    "mini-css-extract-plugin": "0.9.0",
    "morgan": "^1.9.0",
    "pg": "^7.4.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "style-loader": "0.23.1",
    "ts-node-dev": "^1.0.0-pre.63",
    "typescript": "^4.0.3",
    "uuid": "^8.3.0",
    "webpack": "4.41.5",
    "webpack-dev-server": "3.10.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "dotenv": "^8.2.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^4.2.3",
    "nodemon": "^2.0.4",
    "prettier": "^1.19.1",
    "pretty-quick": "^1.5.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [
      "/Users/sidiousvic/Desktop/dev/oats/src"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
